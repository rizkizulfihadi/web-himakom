<div class="body">
    <section class="header galeri">
        <h1>GALLERY</h1>
        <h1>KAMI</h1>
    </section>
    
    <div class="container my-5">
        <!-- <h1 class="title-struktur mb-5">Artikel Terbaru</h1> -->
        <div class="row">   
            <div class="col"></div>
        </div>
    </div>
    
    <section id="gallery">
        <div class="container">
            <div id="image-gallery">
            <div class="row">
                <% galleries.forEach(gallery => { %>
                    <div class="col-lg-3 col-md-6 col-sm-6 col-xs-12 image">
                        <div class="img-wrapper">
                            <a href="/uploads/images/<%= gallery.foto %>"><img src="/uploads/images/<%= gallery.foto %>" class="img-responsive" style="object-fit: cover; height: 150px; "></a>
                            <div class="img-overlay">
                            <i class="fa fa-plus-circle" aria-hidden="true"></i>
                            </div>
                        </div>
                    </div>
                <% }) %>
            </div>
            </div>
        </div>
    </section>

     
    </div>
    